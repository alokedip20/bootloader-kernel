OUTPUT_FORMAT(elf32-i386)	/*WE SET THE EXECUTABLE LINKABLE FORMAT AS 32BIT ELF IS STANDERD BINARY FILE FORMAT FOR UNIX X86 ARCHITECTURE*/
ENTRY(start)			/*	THIS IS THE ENTRY POINT OF THE EXECUTABLE*/
SECTIONS
 {
   . = 0x100000;		/*AS BOOTLOADER LOADS THE KERNEL AT THE LOCATION 0X100000 SO THE (.)LOCATION COUNTER SET TO 0X100000*/
   .text : { *(.text) }		/*THIS MEANS .TEXT AREA IN THE KERNEL.ASM */
   .data : { *(.data) }		/*ALL DATA SECTIONS*/
   .bss  : { *(.bss)  }		/*ALL BSS SECTION*/
 }
